      "types": "./dist/client.d.ts"
    }
  },
  "dependencies": {
    "express": "^4.19.2",
    "fast-glob": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsx": "^4.20.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^4.2.0",
    "chokidar": "^4.0.3",
    "typescript": "^5.6.3",
    "vite": "^5.0.0"
  }
